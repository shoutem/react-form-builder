{
  "name": "@shoutem/react-form-builder",
  "version": "1.0.2",
  "description": "React form builder for Shoutem",
  "author": "Shoutem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://shoutem@github.com/shoutem/react-form-builder.git"
  },
  "bugs": {
    "url": "https://github.com/shoutem/react-form-builder/issues"
  },
  "homepage": "https://github.com/shoutem/react-form-builder#readme",
  "main": "lib/index.js",
  "source": "src/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean-lib": "rimraf ./lib/*",
    "build": "npm run clean-lib && babel src --out-dir lib --copy-files",
    "release": "npm run build && npm publish --access public --tag latest",
    "release-rc": "npm run build && npm publish --access public --tag rc",
    "lint": "eslint --no-eslintrc -c .eslintrc src/**/*.{js,jsx}"
  },
  "dependencies": {
    "@shoutem/react-web-ui": "^0.16.3",
    "auto-bind": "^4.0.0",
    "classnames": "^2.2.6",
    "color-string": "^1.5.3",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.32",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^0.32.4",
    "react-color": "^2.18.0",
    "react-dropzone": "^3.7.4",
    "react-select": "^1.0.0-rc.5",
    "urijs": "^1.19.2",
    "validator": "^12.2.0"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "redux": "^4.0.5",
    "react-redux": "^7.2.4",
    "redux-form": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@shoutem/eslint-config-react": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "rimraf": "^2.5.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "lodash",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
