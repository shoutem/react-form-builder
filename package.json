{
  "name": "react-form-builder",
  "version": "1.0.0",
  "description": "React form builder for Shoutem",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint --no-eslintrc -c .eslintrc src/**/*.{js,jsx}",
    "build": "npm run build-dist && npm run build-lib",
    "clean-dist": "rimraf ./dist/*",
    "clean-lib": "rimraf ./lib/*",
    "build-dist": "npm run clean-dist && webpack --config ./bin/webpack.config.js",
    "build-lib": "npm run clean-lib && babel src --out-dir lib --copy-files",
    "release": "npm run build && npm publish --access public --tag latest"
  },
  "author": "Shoutem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://damirjuretic@github.com/shoutem/react-form-builder.git"
  },
  "bugs": {
    "url": "https://github.com/shoutem/react-form-builder/issues"
  },
  "homepage": "https://github.com/shoutem/react-form-builder#readme",
  "dependencies": {
    "@shoutem/react-web-ui": "^0.16.1-rc.1",
    "auto-bind": "^4.0.0",
    "classnames": "^2.2.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "i18next": "^19.8.2",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.32",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^0.32.4",
    "react-dropzone": "^3.7.4",
    "react-select": "^1.0.0-rc.5",
    "redux": "^4.0.5",
    "redux-form": "^5.4.0"
  },
  "devDependencies": {
    "@shoutem/eslint-config-react": "^1.0.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.8.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.9.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "rimraf": "^2.5.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^5.1.1",
    "prettier": "^1.19.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.19"
  }
}
